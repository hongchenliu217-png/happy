# 环境配置
NODE_ENV=development
PORT=3000

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=your_password
DB_DATABASE=delivery_saas

# Redis配置
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT配置
JWT_SECRET=your_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=7d

# 上游平台API配置
# 美团
MEITUAN_APP_ID=your_meituan_app_id
MEITUAN_APP_SECRET=your_meituan_app_secret
MEITUAN_API_URL=https://api.meituan.com

# 淘宝/饿了么
TAOBAO_APP_KEY=your_taobao_app_key
TAOBAO_APP_SECRET=your_taobao_app_secret
TAOBAO_API_URL=https://eco.taobao.com/router/rest

# 抖音
DOUYIN_APP_ID=your_douyin_app_id
DOUYIN_APP_SECRET=your_douyin_app_secret
DOUYIN_API_URL=https://open.douyin.com

# 下游运力平台API配置
# 达达
DADA_APP_KEY=your_dada_app_key
DADA_APP_SECRET=your_dada_app_secret
